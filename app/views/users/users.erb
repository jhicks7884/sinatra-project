<center><h3>My Deliveries page </h3></center>

<ol>
  <% Delivery.all.each do |deliveries| %>
    <li style="font-size:165%;">
       <%= deliveries.id %>.
       <%= deliveries.name %><br>
       <%= deliveries.address %><br>
       <%= deliveries.content %>

